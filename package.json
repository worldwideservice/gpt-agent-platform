{
  "name": "gpt-agent-platform",
  "version": "1.0.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:demo": "DEMO_MODE=true next dev",
    "test:build": "bash scripts/quick-test.sh",
    "test:platform": "bash scripts/test-platform.sh",
    "generate:openapi": "node scripts/generate-openapi.js",
    "prebuild": "npm run generate:openapi && node scripts/prebuild.js",
    "build": "next build && node scripts/postbuild.js",
    "start": "next start",
    "lint": "next lint",
    "test": "E2E_ONBOARDING_FAKE=1 DEMO_MODE=true playwright test",
    "test:e2e": "E2E_ONBOARDING_FAKE=1 DEMO_MODE=true playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:fast": "E2E_ONBOARDING_FAKE=1 DEMO_MODE=true playwright test --project=chromium --workers=4 --reporter=list",
    "test:quick": "E2E_ONBOARDING_FAKE=1 DEMO_MODE=true playwright test --project=chromium --workers=4 --max-failures=3 --reporter=list",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "playwright:install": "playwright install --with-deps",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "check:env": "node scripts/check-env.js",
    "verify:env": "node scripts/verify-env.js",
    "setup:production": "bash scripts/setup-production.sh",
    "setup:supabase": "bash scripts/setup-supabase.sh",
    "setup:redis": "bash scripts/setup-redis.sh",
    "setup:openrouter": "bash scripts/setup-openrouter.sh",
    "setup:vercel": "bash scripts/setup-vercel-env.sh",
    "deploy:vercel": "bash scripts/deploy-to-vercel.sh",
    "check:redis": "node scripts/check-redis.js",
    "check:worker": "node scripts/check-worker.js",
    "check:all": "bash scripts/check-all-errors.sh",
    "audit:security": "npm audit --audit-level=high",
    "predeploy": "npm run check:all",
    "setup:env": "bash scripts/auto-setup-env.sh",
    "docker:redis": "docker-compose up -d redis",
    "docker:down": "docker-compose down",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "db:migrate": "node scripts/migrations/migrate.js migrate",
    "db:migrate:status": "node scripts/migrations/migrate.js status",
    "db:seed": "node scripts/seed/seed.js run",
    "db:seed:reset": "node scripts/seed/seed.js reset",
    "db:seed:status": "node scripts/seed/seed.js status",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/server": "^5.1.0",
    "@as-integrations/next": "^4.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.62.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.344.0",
    "nanoid": "^5.1.6",
    "next": "^14.2.0",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^19.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swagger-ui-react": "^5.30.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "eslint-plugin-storybook": "^10.0.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.0.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-onboarding": "^10.0.2",
    "@storybook/addon-vitest": "^10.0.2",
    "@storybook/nextjs-vite": "^10.0.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitest/browser-playwright": "^4.0.5",
    "@vitest/coverage-v8": "^4.0.5",
    "playwright": "^1.56.1",
    "storybook": "^10.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vitest": "^4.0.5"
  }
}
